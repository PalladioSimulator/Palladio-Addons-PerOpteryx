    <eClassifiers xsi:type="ecore:EClass" name="StaticSelectionRule" eSuperTypes="#//GDoF/SelectionRule #//GDoF/StaticContextRule"/>
    <eClassifiers xsi:type="ecore:EClass" name="DegreeOfFreedom">
