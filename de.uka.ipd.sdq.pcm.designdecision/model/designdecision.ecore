    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ResourceSelectionDegree" eSuperTypes="#//ClassWithCopyDegree #//ProcessingResourceDegree"/>
  <eClassifiers xsi:type="ecore:EClass" name="StringSetDegree" abstract="true" eSuperTypes="#//UnorderedDegree">
