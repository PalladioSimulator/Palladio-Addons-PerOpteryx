    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HelperOCLDefinition" eSuperTypes="#//GDoF/OCLRule #//GDoF/StaticContextRule">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Defines an additional operation in the context classifier, for use in formulating OCL queries and constraints. The mainOclQuery must be a &quot;def expression&quot;, taking the form of: &#xD;&#xA;&#xD;&#xA;     operation-name(parameters?) : type = expr&#xD;&#xA;&#xD;&#xA;"/>
      </eAnnotations>
