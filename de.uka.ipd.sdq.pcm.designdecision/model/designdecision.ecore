  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OrderedDataTypeDegree" abstract="true"
      eSuperTypes="#//DataTypeDegree"/>
  <eClassifiers xsi:type="ecore:EClass" name="OrderedIntegerDegree" eSuperTypes="#//DiscreteDegree #//OrderedDataTypeDegree">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="listOfIntegers" ordered="false"
