  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_QWjAYs7qEeOX_4BzImuHbA" name="Waiting Time" textualDescription="This measure represents the waiting time, e.g., to store the waiting time at passive resource pools." captureType="RealNumber" scale="Ratio" defaultUnit="ACED00057372001B6A617661782E6D6561737572652E756E69742E42617365556E697400000000000000010200014C00075F73796D626F6C7400124C6A6176612F6C616E672F537472696E673B787200176A617661782E6D6561737572652E756E69742E556E6974CDC8DE1678CD5267020000787074000173"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_CEIUodpkEeO-WvSDaR6unQ" name="Waiting Time Tuple" textualDescription="Specifies a (point in time, waiting time)-tuple, i.e., start of the waiting plus the waiting time." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _QWjAYs7qEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_eg_F0s7qEeOX_4BzImuHbA" name="Resource Demand" textualDescription="This measure represents the CPU/HDD/DELAY/... demand of the entity, e.g., to store CPU demand measurements." captureType="RealNumber" scale="Ratio" defaultUnit="ACED00057372001B6A617661782E6D6561737572652E756E69742E42617365556E697400000000000000010200014C00075F73796D626F6C7400124C6A6176612F6C616E672F537472696E673B787200176A617661782E6D6561737572652E756E69742E556E6974CDC8DE1678CD5267020000787074000173"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_sefjUeJCEeO6l86uYUhhyw" name="Resource Demand Tuple" textualDescription="Specifies a (resource demand, point in time)-tuple, i.e., the resource demand plus the time when it was issued." subsumedMetrics="_eg_F0s7qEeOX_4BzImuHbA _NCRBos7pEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_paDhIs7qEeOX_4BzImuHbA" name="State of Active Resource" textualDescription="This measure represents the state of an active resource, e.g., to store CPU utilization at a certain time/state." captureType="IntegerNumber" scale="Ordinal" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_-buIceJDEeO6l86uYUhhyw" name="State of Active Resource Tuple" textualDescription="Specifies a (point in time, state of active resource)-tuple, i.e., the state of an active resource plus the time when the state was taken." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _paDhIs7qEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_1dsBAeJEEeO6l86uYUhhyw" name="Overall State of Active Resource" textualDescription="Overall state of an active resource, starting from a given point in time. For example, this metric allows to store the utilization of a CPU over the complete simulation time." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _paDhIs7qEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_MSlw0c7sEeOX_4BzImuHbA" name="State of Active Resource over Time" textualDescription="State of an active resource over time, e.g., to store the number of elements in the CPU queue over time." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _paDhIs7qEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_x0-pks7rEeOX_4BzImuHbA" name="State of Passive Resource" textualDescription="This measure represents the number of free tokens in a passive resource, e.g., in a connection pool." captureType="IntegerNumber" scale="Ordinal" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_E-GEoespEeOpcPDUs0BVCg" name="State of Passive Resource Tuple" textualDescription="Specifies a (point in time, state of passive resource)-tuple, i.e., the state of a passive resource plus the time when the state was taken." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _x0-pks7rEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_INq_AespEeOpcPDUs0BVCg" name="Overall State of Passive Resource" textualDescription="Overall state of a passive resource, starting from a given point in time. For example, this metric allows to store the number of connections of a connection pool over the complete simulation time." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _x0-pks7rEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_M_jZoespEeOpcPDUs0BVCg" name="State of Passive Resource over Time" textualDescription="State of a passive resource over time, e.g., to store the number of connections in a connection pool over time." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _x0-pks7rEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_7Is3ss7rEeOX_4BzImuHbA" name="Execution Result" textualDescription="Specifies an execution results metric, e.g., to store simulated failure occurrences." captureType="IntegerNumber" scale="Ordinal" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_3aoZYeJFEeO6l86uYUhhyw" name="Execution Result Tuple" textualDescription="Specifies a (point in time, execution result)-tuple, i.e., time of getting the execution result plus the execution result itself." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _7Is3ss7rEeOX_4BzImuHbA"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_gNxNEtBxEeONzY86HEL4JQ" name="Scalability Range" textualDescription="Scalability range without violating SLOs, e.g., the system scales up to 1,000 users." captureType="IntegerNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_Nz_HctBzEeONzY86HEL4JQ" name="Slope" textualDescription="The slope for increase rates, e.g., an increase rate of 10 users/hour." captureType="RealNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_kx_7UdBzEeONzY86HEL4JQ" name="Scalability Speed" textualDescription="Scalability speed without violating SLOs, e.g., the system scales up to 1,000 users with increase rate 10 users/hour." subsumedMetrics="_gNxNEtBxEeONzY86HEL4JQ _Nz_HctBzEeONzY86HEL4JQ"/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_HlBOUtB0EeONzY86HEL4JQ" name="User Capacity" textualDescription="Maximal number of users for fixed resource without violating SLOs, e.g., a system scales up to 1,000 users for a fixed set of resources." captureType="RealNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_LJJm8tB1EeONzY86HEL4JQ" name="Number of SLO Violations" textualDescription="The number of SLO violations in a certain time frame, e.g., 42 SLO (response time) violations in 1 hour." captureType="IntegerNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_8m5pAtB1EeONzY86HEL4JQ" name="Mean Time to Quality Repair" textualDescription="The mean time to stop violating SLOs, e.g., 30 seconds fir an additional 10 requests/hour." captureType="RealNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_RjNq4tB2EeONzY86HEL4JQ" name="Resource Provisioning Efficiency" textualDescription="Efficiency of provisioning resources, e.g., 10% more resources than actual resource demand." captureType="RealNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_eQICYtB2EeONzY86HEL4JQ" name="Marginal Cost" textualDescription="Marginal cost for serving additional load, e.g., $1.00 for an additional 100 requests/hour." captureType="RealNumber" scale="Ratio" defaultUnit=""/>
  <metricDescriptions xsi:type="metricspec:MetricSetDescription" id="_vBr7MRDZEeSp9ov4gXWqpQ" name="SLOViolations Tuple" textualDescription="Specifies a (point in time, sloviolations)-tuple, i.e., end of the operation call plus the sloviolations." subsumedMetrics="_NCRBos7pEeOX_4BzImuHbA _LJJm8tB1EeONzY86HEL4JQ"/>
</metricspec:MetricDescriptionRepository>
