  <metricDescriptions xsi:type="metricspec:NumericalBaseMetricDescription" id="_zETOUs7pEeOX_4BzImuHbA" name="Holding Time" textualDescription="This measure represents the holding time at a resource, e.g., to store the time spend in passive resource pools." captureType="RealNumber" scale="Ratio" defaultUnit="ACED00057372001B6A617661782E6D6561737572652E756E69742E42617365556E697400000000000000010200014C00075F73796D626F6C7400124C6A6176612F6C616E672F537472696E673B787200176A617661782E6D6561737572652E756E69742E556E6974CDC8DE1678CD5267020000787074000173"/>
