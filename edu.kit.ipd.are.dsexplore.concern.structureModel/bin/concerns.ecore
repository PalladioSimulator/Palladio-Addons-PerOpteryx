<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ConcernModel" nsURI="http://are.ipd.kit.edu/ConcernModel/1.0.1"
    nsPrefix="Concern">
  <eAnnotations source="http://www.eclipse.org/OCL/Import">
    <details key="ecore" value="http://www.eclipse.org/emf/2002/Ecore"/>
    <details key="pcm" value="../../../plugin/org.palladiosimulator.pcm/model/pcm.ecore#/"/>
  </eAnnotations>
  <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
    <details key="invocationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
    <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="ConcernRepository">
    <eStructuralFeatures xsi:type="ecore:EReference" name="concerns" upperBound="-1"
        eType="#//Concern" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Concern">
    <eStructuralFeatures xsi:type="ecore:EReference" name="strategies" upperBound="-1"
        eType="#//ConcernStrategy" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="components" upperBound="-1"
        eType="#//ElementaryConcernComponent" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="annotationtarget" upperBound="-1"
        eType="#//AnnotationTarget" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraints" upperBound="-1"
        eType="#//DeploymentConstraint" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ConcernStrategy">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="concernSolutions" lowerBound="1"
        upperBound="-1" eType="ecore:EClass solution.ecore#//Solution"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElementaryConcernComponent">
    <eStructuralFeatures xsi:type="ecore:EReference" name="requires" upperBound="-1"
        eType="#//ElementaryConcernComponent"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="annotationenrich" upperBound="-1"
        eType="#//AnnotationEnrich" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Annotation" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxAmount" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="0"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AnnotationTarget" eSuperTypes="#//Annotation">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="joinPoint" eType="#//JoinPoint"
        defaultValueLiteral="SIGNATURE"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="signatures" upperBound="-1"
        eType="ecore:EClass platform:/plugin/org.palladiosimulator.pcm/model/pcm.ecore#//repository/Signature"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AnnotationEnrich" eSuperTypes="#//Annotation"/>
  <eClassifiers xsi:type="ecore:EClass" name="DeploymentConstraint">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="isolation togetherSeparated"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="isolation" value="&#xA;&#x9;&#x9;self.association = 'ISOLATED' implies&#xA;&#x9;&#x9;&#x9;self.annotations->size() = 1"/>
      <details key="togetherSeparated" value="&#xA;&#x9;&#x9;self.association = 'TOGETHER' or self.association = 'SEPARATED' implies&#xA;&#x9;&#x9;&#x9;self.annotations->size() > 1"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="association" eType="#//Association"
        defaultValueLiteral="INDIFFERENT"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="annotations" lowerBound="1"
        upperBound="-1" eType="#//Annotation"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Association">
    <eLiterals name="INDIFFERENT"/>
    <eLiterals name="TOGETHER" value="1"/>
    <eLiterals name="ISOLATED" value="2"/>
    <eLiterals name="SEPARATED" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="JoinPoint">
    <eLiterals name="SIGNATURE"/>
    <eLiterals name="INTERFACE" value="1"/>
    <eLiterals name="INTERFACE_REQUIRES" value="2"/>
    <eLiterals name="INTERFACE_PROVIDES" value="3"/>
    <eLiterals name="COMPONENT" value="4"/>
  </eClassifiers>
</ecore:EPackage>
