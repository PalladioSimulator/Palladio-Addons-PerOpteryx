  <eSubpackages name="qualityproperties" nsURI="http://sdq.ipd.uka.de/DesignDecision/QualityProperties/3.0"
      nsPrefix="qualityproperties">
    <eClassifiers xsi:type="ecore:EClass" name="NumericQualityProperty" abstract="true"
        eSuperTypes="#//qualityproperties/QualityProperty">
      <eStructuralFeatures xsi:type="ecore:EReference" name="confidenceInterval" ordered="false"
          eType="#//qualityproperties/ConfidenceInterval"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConfidenceInterval">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="confidenceLevel" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="the confidence level. Use values between 0 and 1. For instance use 0.95 to estimate the 95% confidence interval."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="mean" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="the center of the confidence interval"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="upperBound" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="the confidence interval's upper bound"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="lowerBound" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="the confidence interval's lower bound"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ElementQualityProperty" eSuperTypes="#//qualityproperties/QualityProperty"/>
    <eClassifiers xsi:type="ecore:EClass" name="IntegerQualityProperty" eSuperTypes="#//qualityproperties/NumericQualityProperty">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EIntegerObject"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DoubleQualityProperty" eSuperTypes="#//qualityproperties/NumericQualityProperty">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" ordered="false"
          lowerBound="1" eType="ecore:EDataType ../../../org.eclipse.emf.ecore/model/Ecore.ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QualityPrediction" abstract="true"
