34,382,443,381,449,429,440,430" name="alternativeTo" publish:ismodellink="false" shape="poly" xmi:id="_71117115899767179691006109111101-1177376_675119"/><publish:partInfo coords="377,432,362,384,371,381,386,429,377,432" name="Generalization" publish:ismodellink="false" shape="poly" xmi:id="_707798269767179691006109111101-1177376_675119"/><publish:partInfo coords="401,299,332,196,340,191,409,294,401,299" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_10316101777579101691006101113410579697897911965"/><publish:partInfo coords="549,351,620,367,614,367,676,342,679,352,617,377,617,377,546,361,549,351" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_8211886658757998691006101113410579697897911965"/><publish:partInfo coords="653,315,546,315,546,305,653,305,653,315" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_666410973757989691006101113410579697897911965"/><publish:partInfo coords="123,327,332,327,332,337,123,337,123,327" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_6610984691037578121691006101113410579697897911965"/><publish:partInfo coords="333,303,144,222,147,213,336,294,333,303" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_108114811221077572100691006658376120104115801131057165"/><publish:partInfo coords="463,288,571,149,578,155,470,294,463,288" name="Association" publish:ismodellink="false" shape="poly" xmi:id="_37711510299757299691006658376120104115801131057165"/><publish:partInfo name="Alternative" publish:ismodellink="false" publish:partLowerX="137" publish:partLowerY="467" publish:partUpperX="28" publish:partUpperY="417" shape="rect" xmi:id="_381134817671100691006109111101-1177376_675119"/><publish:partInfo name="AlternativeTask" publish:ismodellink="false" publish:partLowerX="463" publish:partLowerY="479" publish:partUpperX="328" publish:partUpperY="429" shape="rect" xmi:id="_69118121_119767179691006109111101-1177376_675119"/><publish:partInfo name="Edge" publish:ismodellink="false" publish:partLowerX="850" publish:partLowerY="352" publish:partUpperX="654" publish:partUpperY="297" shape="rect" xmi:id="_122118_12085757989691006101113410579697897911965"/><publish:partInfo name="TaskList" publish:ismodellink="false" publish:partLowerX="124" publish:partLowerY="359" publish:partUpperX="28" publish:partUpperY="309" shape="rect" xmi:id="_6584566997578121691006101113410579697897911965"/><publish:partInfo name="Task" publish:ismodellink="false" publish:partLowerX="546" publish:partLowerY="381" publish:partUpperX="333" publish:partUpperY="294" shape="rect" xmi:id="_766710310177757298691006658376120104115801131057165"/></contents><ownedComment annotatedElement="_7410711112165977298691006658376120104115801131057165" body="This is the documentation for the Task and Probabilities metamodel that is the interface for the PerOpteryx metaheuristic optimisation based on the Palladio Component Model to the analytic multicriteria optimisation of Polimi. &#13;&#10;&#13;&#10;This package contains all metamodel elements. The TaskList is the root model element and contains a Task and Probabilities model. &#13;&#10;&#13;&#10;The main class is the Task. It represents a single resource-demanding action in the software. It is derived from actions in the PCM SEFF and can be used for tasks in the optimisation of Polimi. &#13;&#10;&#13;&#10;Tasks are connected to each other by Edges. Each Task can have multiple outgoing Edges. The set of outgoing Edges is modelled with the edge_Task containment association of the Task. Each Edge points to one successor Task. &#13;&#10;&#13;&#10;See the description of the model elements for a detailed description of the attributes. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Comment&gt;" xmi:id="_103901107987668102691006769776778767103777110081" xsi:type="uml:Comment"/><packageImport name="&lt;Unnamed PackageImport&gt;" publish:icon="2688321_icon.jpeg" publish:qualifiedname="TaskModel::&lt;PackageImport&gt;" xmi:id="_7410711112166977298691006658376120104115801131057165" xsi:type="uml:PackageImport"/><packageImport name="&lt;Unnamed PackageImport&gt;" publish:icon="2688321_icon.jpeg" publish:qualifiedname="TaskModel::&lt;PackageImport&gt;" xmi:id="_3827311965757298691006658376120104115801131057165" xsi:type="uml:PackageImport"/><packagedElement name="Task" publish:icon="460077_icon.jpeg" publish:qualifiedname="TaskModel::Task" xmi:id="_766710310177757298691006658376120104115801131057165" xsi:type="uml:Class"><ownedComment annotatedElement="_766710310177757298691006658376120104115801131057165" body="Representation of a task in the performance model. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::&lt;Comment&gt;" xmi:id="_109487789766899691006769776778767103777110081" xsi:type="uml:Comment"/><ownedRule body="self.edge_Task-&gt;collect(e|e.transitionProbability)-&gt;sum()=1 or self.edge_Task-&gt;isEmpty() " constrainedElement="_766710310177757298691006658376120104115801131057165" name="sumOfProbabilitiesMustBeOne" publish:icon="665978417_icon.jpeg" publish:qualifiedname="TaskModel::Task::sumOfProbabilitiesMustBeOne" xmi:id="_88116711098757998691006101113410579697897911965" xsi:type="uml:Constraint"><specification name="sumOfProbabilitiesMustBeOne" publish:icon="52170841_icon.jpeg" publish:qualifiedname="TaskModel::Task::sumOfProbabilitiesMustBeOne::sumOfProbabilitiesMustBeOne" xmi:id="_88116711098977998691006101113410579697897911965" xsi:type="uml:OpaqueExpression"/></ownedRule><ownedComment annotatedElement="_7768866765757298691006658376120104115801131057165" body="Gives the overall probability of execution, which can be derived from the transition probability of the edges leading to this Task. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::probabilityOfExecution::&lt;Comment&gt;" xmi:id="_114119679965766899691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_80671221134757298691006658376120104115801131057165" body="The software failure probability of this task. This does not take into account hardware failures. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::failureProbability::&lt;Comment&gt;" xmi:id="_11812268108103766899691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_82871204111757298691006658376120104115801131057165" body="The mean demanded time that this task demands from the CPU it is deployed on. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::meanDemandedTime::&lt;Comment&gt;" xmi:id="_122_6566103766899691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_37711510299977299691006658376120104115801131057165" body="The PCM component that this Task derives from. This association is needed to determine which tasks can be deployed on which hardware nodes (servers, ResourceContainers). The design decision model defines the allocation of components only. With this link, you can derive the allocation of tasks. Also, this link is needed to map the results of the optmisation back to the PCM model later. Results that allocate two Tasks belonging to the same PCM component to different servers is not valid in the PCM. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::repositorycomponent::&lt;Comment&gt;" xmi:id="_689882661077668108691006886897974121707166581" xsi:type="uml:Comment"/><ownedComment annotatedElement="_108114811221079772100691006658376120104115801131057165" body="This association points to the AllocationContext of this Task the PCM model. Using it, you can determine the ResourceContainer (i.e. hardware node, the server) that this Task is deployed on. Use myTask.getAllocationcontext.getResourceContainer_AllocationContext to retrieve the hardware node. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::allocationcontext::&lt;Comment&gt;" xmi:id="_6611611267737668108691006886897974121707166581" xsi:type="uml:Comment"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::Task::allocationcontext::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_10811481122107672100691006658376120104115801131057165" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::Task::allocationcontext::&lt;LiteralInteger&gt;" xmi:id="_1081148112210711372100691006658376120104115801131057165" xsi:type="uml:LiteralInteger"/><ownedComment annotatedElement="_821181011208757998691006101113410579697897911965" body="The outgoing Edges of this Task. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::edge_Task::&lt;Comment&gt;" xmi:id="__12282110777668107691006886897974121707166581" xsi:type="uml:Comment"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::Task::edge_Task::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_8211810112081137998691006101113410579697897911965" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::Task::edge_Task::&lt;LiteralInteger&gt;" xmi:id="_821181011208977998691006101113410579697897911965" xsi:type="uml:LiteralInteger"/><ownedComment annotatedElement="_10316101779779101691006101113410579697897911965" body="The SEFF action that this Task was transformed from. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Task::abstractaction::&lt;Comment&gt;" xmi:id="_6797118891157668108691006886897974121707166581" xsi:type="uml:Comment"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::Task::abstractaction::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_1031610177679101691006101113410579697897911965" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::Task::abstractaction::&lt;LiteralInteger&gt;" xmi:id="_103161017711379101691006101113410579697897911965" xsi:type="uml:LiteralInteger"/></packagedElement><packagedElement name="&lt;Unnamed Association&gt;" publish:icon="51120219_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;" xmi:id="_37711510299757299691006658376120104115801131057165" xsi:type="uml:Association"><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;::&lt;Property&gt;::&lt;LiteralUnlimitedNatural&gt;" xmi:id="__8182789977299691006658376120104115801131057165" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;::&lt;Property&gt;::&lt;LiteralInteger&gt;" xmi:id="__8182789757299691006658376120104115801131057165" xsi:type="uml:LiteralInteger"/></packagedElement><packagedElement name="&lt;Unnamed Association&gt;" publish:icon="51120219_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;" xmi:id="_108114811221077572100691006658376120104115801131057165" xsi:type="uml:Association"><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;::&lt;Property&gt;::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_1081148112210811372100691006658376120104115801131057165" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;::&lt;Property&gt;::&lt;LiteralInteger&gt;" xmi:id="_108114811221089772100691006658376120104115801131057165" xsi:type="uml:LiteralInteger"/></packagedElement><packagedElement name="TaskList" publish:icon="460077_icon.jpeg" publish:qualifiedname="TaskModel::TaskList" xmi:id="_6584566997578121691006101113410579697897911965" xsi:type="uml:Class"><ownedComment annotatedElement="_6584566997578121691006101113410579697897911965" body="The root model element that contains all Tasks of this model. Additionally, it contains a set of Alternatives, that contain AlternativeTasks to model component selection options. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::&lt;Comment&gt;" xmi:id="_11710873107897668100691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_6610984691039778121691006101113410579697897911965" body="The set of Tasks in the model. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::task_TaskList::&lt;Comment&gt;" xmi:id="_6511979881117668108691006886897974121707166581" xsi:type="uml:Comment"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::task_TaskList::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_661098469103678121691006101113410579697897911965" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::task_TaskList::&lt;LiteralInteger&gt;" xmi:id="_66109846910311378121691006101113410579697897911965" xsi:type="uml:LiteralInteger"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::startTask_TaskList::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_7867611510377179691006109111101-1177376_675119" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::startTask_TaskList::&lt;LiteralInteger&gt;" xmi:id="_786761151031147179691006109111101-1177376_675119" xsi:type="uml:LiteralInteger"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::alternative_TaskList::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_5118799865771100691006109111101-1177376_675119" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::TaskList::alternative_TaskList::&lt;LiteralInteger&gt;" xmi:id="_511879986511471100691006109111101-1177376_675119" xsi:type="uml:LiteralInteger"/></packagedElement><packagedElement name="&lt;Unnamed Association&gt;" publish:icon="919965_icon.jpeg" publish:qualifiedname="TaskModel::&lt;Association&gt;" xmi:id="_6610984691037578121691006101113410579697897911965" xsi:type="uml:Association"/><packagedElement name="Edge" publish:icon="460077_icon.jpeg" publish:qualifiedname="TaskModel::Edge" xmi:id="_122118_12085757989691006101113410579697897911965" xsi:type="uml:Class"><ownedComment annotatedElement="_122118_12085757989691006101113410579697897911965" body="An Edge connects two Tasks. Each Task has a set of Edges, that themselves point to one successor Task each. For example, for a task with three successor tasks, you have three Edges, each pointing to one of the successors. &#13;&#10;" isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Edge::&lt;Comment&gt;" xmi:id="_7210710611407668100691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_088778577757989691006101113410579697897911965" body="The transition probability of this Task. The transition probabilities of all Edges exiting one Task usually sum up to 1. If they do not, this is an implicit stop task, i.e. the control flow ends here with the remaining probability. TODO: Add OCL that sum of probabilities must not be larger than 1. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Edge::transitionProbability::&lt;Comment&gt;" xmi:id="_8012167681077668100691006769776778767103777110081" xsi:type="uml:Comment"/><ownedComment annotatedElement="_666410973977989691006101113410579697897911965" body="This association points to the successor Task of this Edge. " isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="1091439591_icon.jpeg" publish:qualifiedname="TaskModel::Edge::successor_Edge::&lt;Comment&gt;" xmi:id="_-5_791199868107691006886897974121707166581" xsi:type="uml:Comment"/><upperValue name="&lt;Unnamed LiteralUnlimitedNatural&gt;" publish:icon="17040471_icon.jpeg" publish:qualifiedname="TaskModel::Edge::successor_Edge::&lt;LiteralUnlimitedNatural&gt;" xmi:id="_66641097367989691006101113410579697897911965" xsi:type="uml:LiteralUnlimitedNatural"/><lowerValue name="&lt;Unnamed LiteralInteger&gt;" publish:icon="31855319_icon.jpeg" publish:qualifiedname="TaskModel::Edge::successor_Edge::&lt;LiteralInteger&gt;" xmi:id="_6664109731137989691006101113410579697897911965" xsi:type="uml:LiteralInteger"/></packagedElement><packagedElement name="&lt;Unnamed